<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    <title>{% block title %}Welcome!{% endblock %}</title>
    {% block stylesheets %}{% endblock %}
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link rel="stylesheet" href="{{ asset('materialize/css/materialize.min.css') }}">
    <style type="text/css">
    	/* ---- reset ---- */ body{ margin:0;} /* ---- particles.js container ---- */ #particles-js{ position:absolute; top: 0; z-index: 0; width: 100%; height: 100vh!important; background-color: #90a4ae; background-image: url(""); background-repeat: no-repeat; background-size: cover; background-position: 50% 50%; } 
    </style>
</head>
<body class="grey lighten-2">
{% block body %}{% endblock %}
</body>
<script type="text/javascript" src="{{ asset('jquery-3.2.1.min.js') }}"></script>
<script src="{{ asset('materialize/js/materialize.min.js') }}"></script>
{% block javascripts %}{% endblock %}
<script type="text/javascript">
    $(document).ready(function(){
        $('.tooltipped').tooltip({delay: 50});
    });
</script>
</html>
